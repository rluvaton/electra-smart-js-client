{
  "name": "electra-smart-js-client",
  "version": "0.0.0-development",
  "description": "Electra Smart JS Client",
  "keywords": [
    "electra",
    "smart",
    "ac",
    "air conditioning",
    "IOT"
  ],
  "files": [
    "dist"
  ],
  "bin": {
    "electra-smart-js-client": "./dist/index.js"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf lib",
    "start": "ts-node src/index.ts",
    "coverage": "jest --coverage",
    "test": "jest --passWithNoTests",
    "watch": "jest --watch",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@jest/types": "^27.1.1",
    "@types/faker": "^5.5.9",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-extended": "^1.2.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.27.2"
  },
  "author": "Raz Luvaton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rluvaton/electra-smart-js-client/issues"
  },
  "homepage": "https://github.com/rluvaton/electra-smart-js-client#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rluvaton/electra-smart-js-client.git"
  }
}
